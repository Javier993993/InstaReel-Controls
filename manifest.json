{
  "manifest_version": 3,
  "name": "InstaReel Controls",
  "version": "0.1.0",
  "description": "Better web controls for Instagram Reels: volume popover, progress bar, timestamp sharing.",
  "action": {
    "default_title": "InstaReel Controls"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ],
      "run_at": "document_start"
    }
  ]
}
